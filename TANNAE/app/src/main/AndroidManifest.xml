        <activity android:name=".activity.main_service.ServiceReqActivity"/>
        <activity android:name=".activity.main_service.NavigationActivity"/>
        <activity android:name=".activity.main_service.PaymentActivity"/>

        <activity android:name=".activity.user_service.LostFoundActivity"/>
        <activity android:name=".activity.user_service.HistoryActivity"/>
        <activity android:name=".activity.user_service.PointActivity"/>
        <activity android:name=".activity.user_service.QnAActivity"/>
        <activity android:name=".activity.user_service.QnADetailActivity"/>
        <activity android:name=".activity.user_service.QnAEditActivity"/>
        <activity android:name=".activity.user_service.FAQActivity"/>
        <activity android:name=".activity.user_service.FAQEditActivity"/>

        <meta-data android:name="com.kakao.sdk.AppKey" android:value="9baed5ab6907b4f6a2a5ce8fb2a0df03"/>
    </application>

</manifest>
