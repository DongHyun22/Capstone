        </activity>

        <activity android:name=".activity.account.AccountActivity"/>
        <activity android:name=".activity.account.AccountEditActivity"/>
        <activity android:name=".activity.account.SignUpActivity"/>
        <activity android:name=".activity.account.FindActivity"/>

